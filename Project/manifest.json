{
    "name": "Text Font Size",
    "version": "1.0",
    "description": "Allows the user to choose the font size for any text on a web page.",
    "manifest_version": 3,
    "permissions": ["storage"],
    "options_ui": {
      "page": "options.html"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]

    }]
  }
  